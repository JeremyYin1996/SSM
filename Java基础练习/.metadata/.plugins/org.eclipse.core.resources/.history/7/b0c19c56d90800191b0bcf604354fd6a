import java.util.Scanner;

/**
 * @author Jeremy
 * @date 2018-12-26
 * @time 下午2:37:11
 */

public class Test5 {
public static void main(String[] args) {
	Scanner scanner = new Scanner(System.in);
	System.out.println("请输入小红考试分数：");
	int point = scanner.nextInt();
	if(point>90){
		System.out.println("玩游戏一天");
	}else{
		System.out.println("打扫厕所");
	}
}
}


